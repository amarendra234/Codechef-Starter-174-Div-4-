#include <bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        string str;
        cin>>str;
        string ans="";
        int f=0;
        for(int i=0;i<n;i++){
           ans+='z';
        }
        if(ans!=str){
            cout<<ans<<endl;
        }else{
            cout<<-1<<endl;
        }

    }
}
